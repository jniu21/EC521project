{
    "manifest_version": "3",
    "name": "Phishing Detector",
    "version": "0.1",
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "action": {
        "default_popup": "popup/popup.html"
    },
    "background": {
        "service_worker": "scripts/background.js"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self' 'wasm-unsave-eval'; object-src 'self'"
    }
}