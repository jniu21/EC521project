{
    "manifest_version": 3,
    "name": "Phishing Detector",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "action": {
        "default_popup": "popup/popup.html"
    },
    "background": {
        "service_worker": "scripts/background.js"
    }
}